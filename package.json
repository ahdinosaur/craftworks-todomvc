{
  "name": "craftworks-todomvc",
  "version": "0.0.0",
  "description": "real-world production-quality TodoMVC example",
  "main": "index.js",
  "scripts": {
    "postinstall": "lnfs app node_modules/app",
    "lint": "snazzy",
    "format": "snazzy --format",
    "test": "npm-run-all -p test:*",
    "test:spec": "node spec",
    "test:feature": "node feature",
    "dev:client": "watchify client -o assets/bundle.js -dv",
    "dev:assets": "cpx \"app/assets/**/*\" assets -w",
    "dev:server": "node-dev server",
    "prod:client": "browserify client -o assets/bundle.js -g envify -g uglifyify",
    "prod:assets": "cpx \"app/assets/**/*\" assets",
    "prod:server": "node server",
    "dev": "NODE_ENV=development npm-run-all -p dev:*",
    "prod": "NODE_ENV=production npm-run-all -s prod:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/craftworks-todomvc.git"
  },
  "keywords": [
    "todomvc",
    "example",
    "react",
    "redux",
    "frp",
    "functional",
    "reactive",
    "immutable",
    "feathers",
    "ddd",
    "tcomb"
  ],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahdinosaur/craftworks-todomvc/issues"
  },
  "homepage": "https://github.com/ahdinosaur/craftworks-todomvc#readme",
  "engines": {
    "node": "^4.0.0",
    "npm": "^3.0.0"
  },
  "devDependencies": {
    "cuke-tap": "^1.0.2",
    "jsdom": "^7.1.0",
    "node-dev": "^2.7.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-logger": "^2.0.4",
    "tape": "^4.2.2",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "babel-core": "^6.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-runtime": "^6.2.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "bulk-require": "^0.2.1",
    "bulkify": "^1.1.1",
    "cpx": "^1.2.1",
    "envify": "^3.4.0",
    "evalify": "^1.0.1",
    "feathers": "^1.2.0",
    "history": "^1.13.1",
    "lnfs-cli": "^1.0.1",
    "npm-run-all": "^1.3.2",
    "pinkie-promise": "^2.0.0",
    "ramda": "^0.18.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-simple-router": "0.0.10",
    "redux-thunk": "^1.0.0",
    "sheetify": "^3.1.0",
    "uglifyify": "^3.0.1"
  }
}
